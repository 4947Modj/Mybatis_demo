<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Mapper.PersonMapper">

    <insert id="addPerson">
        insert into jdbc_demo.person
        values (2, 'tom', 20, 1)
    </insert>
    <select id="getPerson" resultType="POJO.Person">
        SELECT * FROM jdbc_demo.person where id = 1;
    </select>

</mapper>